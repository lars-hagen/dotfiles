{
    "title": "Open Terminal",
    "rules": [
        {
            "description": "Control + Option + Command + T to open Terminal",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "modifiers": {
                            "mandatory": [
                                "control",
                                "option",
                                "command"
                            ]
                        },
                        "key_code": "t"
                    },
                    "to": [
                        {
                            "shell_command": "open -a Terminal"
                        }
                    ]
                }
            ]
        }
    ]
}

